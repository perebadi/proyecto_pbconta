<p id="notice"><%= notice %></p>

<h1>Listing Facturas</h1>

<table>
  <thead>
    <tr>
      <th>Id cliente</th>
      <th>Fecha factura</th>
      <th>Base imponible</th>
      <th>Descuento</th>
      <th>Iva</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facturas.each do |factura| %>
      <tr>
        <td><%= factura.id_cliente %></td>
        <td><%= factura.fecha_factura %></td>
        <td><%= factura.base_imponible %></td>
        <td><%= factura.descuento %></td>
        <td><%= factura.iva %></td>
        <td><%= factura.total %></td>
        <td><%= link_to 'Show', factura %></td>
        <td><%= link_to 'Edit', edit_factura_path(factura) %></td>
        <td><%= link_to 'Destroy', factura, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Factura', new_factura_path %>
