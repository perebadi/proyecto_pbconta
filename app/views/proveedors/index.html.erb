<p id="notice"><%= notice %></p>

<h1>Listing Proveedors</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Contacto</th>
      <th>Cargo contacto</th>
      <th>Direccion</th>
      <th>Id ciudad</th>
      <th>Id provincia</th>
      <th>Id pais</th>
      <th>Cp</th>
      <th>Telefono</th>
      <th>Movil</th>
      <th>Email</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedors.each do |proveedor| %>
      <tr>
        <td><%= proveedor.nombre %></td>
        <td><%= proveedor.contacto %></td>
        <td><%= proveedor.cargo_contacto %></td>
        <td><%= proveedor.direccion %></td>
        <td><%= proveedor.id_ciudad %></td>
        <td><%= proveedor.id_provincia %></td>
        <td><%= proveedor.id_pais %></td>
        <td><%= proveedor.cp %></td>
        <td><%= proveedor.telefono %></td>
        <td><%= proveedor.movil %></td>
        <td><%= proveedor.email %></td>
        <td><%= proveedor.url %></td>
        <td><%= link_to 'Show', proveedor %></td>
        <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
        <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveedor', new_proveedor_path %>
